<div class="feature-item">
	<a href="/blog/{{ post.slug }}" style="background:url('{{ getMediaImage(post.image).src }}') center center; background-size:cover;" target="_blank">
		<h2>{{ post.title }}<span>{{ post.excerpt }}</span></h2>
		<div class="description">
			<p>{{ post.metaDescription }}</p>
		</div>
	</a>
</div>